\ProvidesPackage{uqtrbeamercolors}
\RequirePackage{uqtrcolors}


% --------------------------------------------------------------------------- %
% Selection
% --------------------------------------------------------------------------- %


\def \colorA {UQTRvert}
\def \colorB {White}
\def \colorC {PaleYellow}
\def \colorE {UQTRvert}
\def \colorD {UQTRor}


% --------------------------------------------------------------------------- %
% Pattern
% --------------------------------------------------------------------------- %

\setbeamercolor{block title}        {bg=\colorE,fg=\colorB}
\setbeamercolor{block body}         {bg=\colorC,fg=black}

\setbeamercolor{itemize item}       {fg=\colorA}
\setbeamercolor{itemize subitem}    {fg=\colorA}
\setbeamercolor{title}              {bg=\colorA,fg=\colorB}
\setbeamercolor{frametitle}         {bg=\colorA,fg=\colorB}

\setbeamercolor{section number projected}{bg=\colorA,fg=\colorB}
\setbeamercolor{section in toc}{fg=black}

\setbeamercolor{palette primary}    {bg=\colorD,fg=\colorA}
\setbeamercolor{palette secondary}  {bg=\colorA,fg=\colorA}
\setbeamercolor{palette tertiary}   {bg=\colorE,fg=\colorA}
\setbeamercolor{palette quaternary} {bg=\colorE,fg=\colorA}
\setbeamercolor{background canvas}  {bg=\colorB}

\setbeamercolor{structure}          {bg=\colorD,fg=\colorA}
\setbeamercolor{framesubtitle}      {bg=\colorD,fg=\colorA}
\setbeamercolor{headline}           {bg=\colorD,fg=\colorA}



